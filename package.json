  "license": "MIT",
  "scripts": {
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "start": "nps",
    "test:lint-fix": "yarn test:stylelint && nps test.lint.fix",
    "test:lint": "yarn test:stylelint && nps test.lint",
    "test": "yarn test:lint && yarn test:jest",
    "test:local": "yarn test:lint-fix && yarn test:jest && yarn cc",
    "test:jest": "cross-env NODE_ENV=test && jest --no-cache --config jest.config.json --notify",
    "protest": "nps test",
    "test:jest-react-u": "nps test.react.accept",
    "installglobals": "npm i -g npm@next",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && yarn cache clean && yarn install",
    "snyk-protect": "snyk protect",
    "eslint:fix": "./node_modules/eslint/bin/eslint.js --fix .",
    "postinstall": "yarn start -- build",
    "protractor": "nps e2e",
    "test:stylelint": "stylelint \"static/**/*.css\""
  },
